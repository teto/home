{
    "workspaces" : {
        "ws1" : "$mod+ampersand",
        "ws2" : "$mod+eacute",
        "ws2" : "$mod+quotedbl",
        "ws2" : "$mod+apostrophe",
        "ws2" : "$mod+parenleft",
        "ws2" : "$mod+minus",
        "ws2" : "$mod+egrave",
        "ws2" : "$mod+underscore",
        "ws2" : "$mod+ccedilla"

    },

    "modes/resize" : {

    },
    "modes/monitors" : {
        "enable_mode" : "$mod+m",
        "leave mode" : "escape",
        "prev_on_output" : "$mod+Shift+Left"
    },


    "config.tpl" : {
        "ipc-socket" : "~/.i3/ipc.sock",
        "modifier" :  "Mod1",
        "terminals" : {
            "urxvt-unicode" : "",
            "launch" : " -name %s -e "
        },
        "plugins" : [ "volume.alsa", "xbacklight","players.ncmpcpp" ]
    },
    "macros/dunst.tpl" : {
    },

    "keys/focus" : {
        "arrows" : {
            "focus_left" : "$mod+Left",
            "focus_right" : "$mod+Right",
            "focus_up" : "$mod+Up",
            "focus_down" : "$mod+Down"
        },
        "jklm" : {
            "focus_left" : "$mod+j",
            "focus_right" : "$mod+m",
            "focus_up" : "$mod+k",
            "focus_down" : "$mod+l"
        }
    },
    "keys/move_win" : {
        "arrows" : {
            "move_left" : "$mod+Shift+Left",
            "move_right": "$mod+Shift+Right",
            "move_down": "$mod+Shift+Down",
            "move_up": "$mod+Shift+Up"
        },
        "jklm": {
            "move_left" : "$mod+Shift+K",
            "move_right": "$mod+Shift+M",
            "move_down": "$mod+Shift+K",
            "move_up": "$mod+Shift+J"
        }
    },
    "volume" : {
        "alsa" : {	
        	"volume_up" : "amixer -q set Master 2dB+ unmute",
        	"volume_down" : "amixer -q set Master 2dB- unmute",
            "volume_mute" : "amixer -q set Master toggle"
        },
        "pulseaudio" : {
            "volume_up"  : "pacmd set-sink-volume 1",
            "volume_down" : " pacmd set-sink-volume 1",
            "volume_mute" : ""
        }
    },


    "xbacklight" : {
        "lightup" : "xbacklight -inc 10",
        "lightdown" : "xbacklight -dec 5"
    },
    "keys/mediaplayer" : {
        "mocp" : {
            "pause" :  "mocp -P",
            "stop" :  "mocp -P",
            "play" : "mocp -G",
            "next" : "mocp -f",
            "prev" : "mocp -r"
        },
        "cmus" : {
            "pause" :  "mocp -P",
            "stop" :  "mocp -P",
            "play" : "mocp -G",
            "next" : "mocp -f",
            "prev" : "mocp -r"
        },
        "ncmpcpp" : {
            "stop" :  "ncmcpp stop",
            "pause" :  "ncmcpp pause",
            "play" : "ncmcpp toggle",
            "next" : "ncmcpp next",
            "prev" : "ncmpcpp previous"
        }
    }

}

