[colors]

# special
foreground        = #f8f8f2
foreground_bold   = #f8f8f2
cursor            = #f8f8f2
cursor_foreground = #272822
background        = rgba(39, 40, 34, 0.8)

# black
color0  = #272822
color8  = #75715e

# red
color1  = #f92672
color9  = #f92672

# green
color2  = #a6e22e
color10 = #a6e22e

# yellow
color3  = #f4bf75
color11 = #f4bf75

# blue
color4  = #66d9ef
color12 = #66d9ef

# magenta
color5  = #ae81ff
color13 = #ae81ff

# cyan
color6  = #a1efe4
color14 = #a1efe4

# white
color7  = #f8f8f2
color15 = #f9f8f5

[options]
scroll_on_output = false
scroll_on_keystroke = true
audible_bell = false
mouse_autohide = false
allow_bold = true
dynamic_title = true
fullscreen = false
urgent_on_bell = true
scrollbar=right
clickable_url = false

# apparently it's possible to
font = Inconsolata 11
# font = FontAwesome 11
scrollback_lines = 1000000
search_wrap = false
#icon_name = terminal
#geometry = 640x480

# "system", "on" or "off"
cursor_blink = system

# "block", "underline" or "ibeam"
cursor_shape = block

# $BROWSER is used by default if set, with xdg-open as a fallback
# browser = qutebrowser

# set size hints for the window
#size_hints = false

# Hide links that are no longer valid in url select overlay mode
filter_unmatched_urls = true

# emit escape sequences for other keys modified by Control
#modify_other_keys = false



# Solarized dark color scheme

